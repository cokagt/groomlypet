{
    "name": "Review",
    "type": "object",
    "properties": {
        "user_email": {
            "type": "string",
            "description": "Email del usuario que deja la rese\u00f1a"
        },
        "business_id": {
            "type": "string",
            "description": "ID del negocio"
        },
        "appointment_id": {
            "type": "string",
            "description": "ID de la cita relacionada"
        },
        "rating": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "description": "Calificaci\u00f3n general de 1 a 5 estrellas"
        },
        "comment": {
            "type": "string",
            "description": "Comentario de la rese\u00f1a"
        },
        "service_quality": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "description": "Calificaci\u00f3n de calidad del servicio"
        },
        "cleanliness": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "description": "Calificaci\u00f3n de limpieza"
        },
        "staff_friendliness": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "description": "Calificaci\u00f3n de amabilidad del personal"
        },
        "punctuality": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "description": "Calificaci\u00f3n de puntualidad"
        },
        "order": {
            "type": "number",
            "minimum": 1,
            "maximum": 5,
            "description": "Calificaci\u00f3n de orden y organizaci\u00f3n"
        }
    },
    "required": [
        "user_email",
        "business_id",
        "rating"
    ]
